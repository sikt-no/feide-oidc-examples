{
  "pythonVersion": "3.14",
  "typeCheckingMode": "strict",
  "venvPath": ".",
  "venv": ".venv",
  "include": ["src", "tests"],
  "reportMissingTypeStubs": false,
  "reportUnknownMemberType": true,
  "reportUnknownArgumentType": true,
  "reportUnknownVariableType": true,
  "reportImplicitOverride": true,
  "reportUnusedCallResult": true
}